{
  "name": "dtsgenerator-express-route-types",
  "version": "0.2.3",
  "description": "plugin for generating types for Express route handlers",
  "author": {
    "name": "rjbma"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.ts **/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "precompile": "npm run lint",
    "compile": "tsc -p .",
    "watch": "npm run compile -- -w",
    "build": "npm run compile",
    "do-test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/*_test.ts",
    "test": "nyc npm run do-test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:update-snapshot": "UPDATE_SNAPSHOT=1 npm run do-test"
  },
  "keywords": [
    "dtsgenerator",
    "dtsgenerator-plugin"
  ],
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cross-env": "^7.0.3",
    "dtsgenerator": "^3.13.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
