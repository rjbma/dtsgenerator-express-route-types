{
  "name": "dtsgenerator-express-route-types",
  "version": "0.2.8",
  "description": "plugin for generating types for Express route handlers",
  "author": {
    "name": "rjbma"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.ts **/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "precompile": "npm run lint",
    "compile": "tsc -p .",
    "watch": "npm run compile -- -w",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "do-test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/*_test.ts",
    "test": "nyc npm run do-test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:update-snapshot": "UPDATE_SNAPSHOT=1 npm run do-test"
  },
  "keywords": [
    "dtsgenerator",
    "dtsgenerator-plugin"
  ],
  "dependencies": {
    "tslib": "^2.5.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "cross-env": "^7.0.3",
    "dtsgenerator": "^3.18.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1"
  }
}
